{
  "name": "observable-process",
  "version": "8.0.0-pre4",
  "description": "High-level support for running, observing, and interacting with child processes in Node.js",
  "homepage": "https://github.com/Originate/observable-process",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Originate/observable-process.git"
  },
  "license": "ISC",
  "author": "Kevin Goslar",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "clean": "rm -rf dist",
    "coverage": "node --test --import tsx --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info -- test/*.test.ts",
    "fix": "eslint . --fix --ext .ts --ignore-path .eslintignore && prettier --write . && sort-package-json",
    "lint": "eslint . --ext .ts --ignore-path .eslintignore && prettier -l . && sort-package-json --check",
    "prepublishOnly": "npm run build",
    "setup": "npm install",
    "test": " npm run build && npm run lint && npm run unit",
    "unit": "node --test --import tsx src/*.test.ts",
    "update": "npm-check-updates -u && npm install"
  },
  "dependencies": {
    "merge-stream": "2.0.0",
    "string-argv": "0.3.1",
    "text-stream-search": "4.0.3"
  },
  "devDependencies": {
    "@types/merge-stream": "1.1.2",
    "@types/node": "22.13.10",
    "@types/string-argv": "0.3.0",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "eslint": "7.28.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-typescript-sort-keys": "1.6.0",
    "portfinder": "1.0.28",
    "prettier": "2.3.1",
    "sort-package-json": "1.57.0",
    "source-map-support": "0.5.19",
    "text-runner": "7.1.0",
    "textrun-npm": "0.3.0",
    "tsx": "^4.19.3",
    "typescript": "4.3.2"
  }
}
